    "version": "0.2.0",
    "configurations": [
        {
            "name": "msdetr",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd":"/root/workspace/MS-DETR",
            "env": {"code_version":"debug"},
            "args": [
                    "--lr", "5e-5",
                    "--lr_backbone", "5e-6",
                    // "--backbone", "resnet50",
                    "--backbone", "resnet18",
                    // "--batch_size", "4",
                    "--enc_layers", "6",
                    "--dec_layers", "6",
                    "--output_dir", "output",
                    "--with_box_refine", 
                    "--two_stage", 
                    "--dim_feedforward", "2048",
                    "--epochs", "12",
                    "--lr_drop", "11",
                    "--num_queries", "300",
                    "--dropout", "0.0",
                    "--mixed_selection", 
                    "--look_forward_twice", 
                    "--use_ms_detr", 
                    "--use_aux_ffn", 
                    "--cls_loss_coef", "1",
                    "--o2m_cls_loss_coef", "2",
                    "--enc_cls_loss_coef", "2",
                    "--enc_bbox_loss_coef", "5",
                    "--enc_giou_loss_coef", "2",

                    // "--wandb_enabled"

                //    "--resume", "exps/contrastive_loss_1st_try-default_size-homeworkstation-minicoco-resnet18_enc6_dec6_query300-bs2x1_lr1e-4/checkpoint.pth",
                //    "--eval",
                 ],
        }
    ]

}